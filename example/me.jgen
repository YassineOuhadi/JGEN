
project me

    entity asUser
	    attribute id type Integer
		attribute fullname type String

	relationship OneToMany from asUser to asUser

	configuration
		metadata
			buildTool Maven
			springVersion 3.1.6
			group com.example
			artifact me
			name me
			description "Demo project for Spring Boot"
			package com.example.me
			packaging Jar
			javaVersion 17

		datasource
			type MySQL
			host localhost
			port 3306
			database db

		server
			host localhost
			port 8080
